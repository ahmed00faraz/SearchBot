{
    "manifest_version": 3,
    "name": "BOT",
    "version": "1.0",
    "permissions": ["activeTab", "storage","tabs"],
    "action": {
        "default_title": "My Extension",
        "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
}
  